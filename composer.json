{
    "name": "osamah/laravel-dynamic-integrations",
    "description": "A Laravel package for creating dynamic API integrations with artisan commands and facades",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "laravel",
        "integration",
        "api",
        "dynamic",
        "facade",
        "artisan",
        "gcp",
        "rest"
    ],
    "authors": [
        {
            "name": "Osamah",
            "email": "your-email@example.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/support": "^10.0|^11.0",
        "illuminate/console": "^10.0|^11.0",
        "illuminate/http": "^10.0|^11.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0",
        "orchestra/testbench": "^8.0|^9.0"
    },
    "autoload": {
        "psr-4": {
            "Osamah\\LaravelDynamicIntegrations\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Osamah\\LaravelDynamicIntegrations\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Osamah\\LaravelDynamicIntegrations\\LaravelDynamicIntegrationsServiceProvider"
            ],
            "aliases": {
                "Integration": "Osamah\\LaravelDynamicIntegrations\\Facades\\Integration"
            }
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
